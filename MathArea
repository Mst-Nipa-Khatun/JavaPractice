
package com.mycompany.selfpractice;

public class Exc13 {
    public static void main(String[] strings) {
         double width = 5.6,  height = 8.5;
         double perimeter, area;

        
        perimeter = 2 * (height + width);
	System.out.println("Perimeter: " +perimeter); // 2*(%.1f + %.1f) = %.2f \n;
	
        
        area = width * height;			
        System.out.println("Area: " +area); // %.1f * %.1f = %.2f \n
        
    }
    
}
